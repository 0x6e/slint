TestCase := Rectangle {
    width: 800px;
    height: 800px;

    Path {
        commands: "M 400 300 A 50 50 0 1 1 550 300 C 550 400 450 450 250 450";
        stroke_width: 2;
        stroke_color: black;
    }

    PathLayout {

        commands: "M 400 300 A 50 50 0 1 1 550 300 C 550 400 450 450 250 450";

        offset: 0;

        animate offset {
            duration: 1000ms;
            loop_count: 4;
        }

        Text {
            text: "Moving around";
            color: black;
        }

        Rectangle {
            width: 20px;
            height: 20px;
            color: green;
        }

        Text {
            text: "Blah";
            color: black;
        }

    }
}
