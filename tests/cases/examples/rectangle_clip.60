// This should render as a rectangle with a black border and
// green "fill".

export Test := Window {
    background: white;
    Rectangle {
        x: 50px;
        y: 50px;
        width: 200px;
        height: 200px;
        border-radius: 25px;
        border-width: 10px;
        border-color: black;

        clip: true;

        Rectangle {
            x: -20px;
            y: -20px;
            width: parent.width + 20px;
            height: parent.height + 20px;
            background: green;
        }

        Rectangle {
            x: 150px;
            y: 150px;
            width: 200px;
            height: 200px;
            background: red;
        }
    }
}