
TestCase := Rectangle {
    width: 100px;
    height: 100px;
    color: white;
    property<int32> top_level: 42;

   for pp[idx] in 5: Rectangle {
        s := Rectangle {
            property<int32> within: 88;
            x: 2px * idx;
            y: 200px * pp;
            width: s.within;
            height: root.top_level;
            for nested in [1px] : Rectangle {
                x : s.width + root.top_level + nested;
            }
        }
    }

    for pp[idx] in [1,3,2]: Rectangle {
        x: idx;
        y: 25px * pp;
    }

    for pp[idx] in ["1","3","2"]: Rectangle {
        x: idx;
        Text { text: pp; }
    }

    for pp in [{a: 12, b: "aa", c: {a: #00f}}, {a: 13, b: "cc", c: { a: #f00}}]: Text {
        x: pp.a;
        text: pp.b;
        color: pp.c.a;
    }
}
