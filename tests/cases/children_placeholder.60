Container := Rectangle {
    GridLayout {
        Row {
            Rectangle {
                color: green;
            }
            $children
        }
    }
}

TestCase := Container {
    width: 300px;
    height: 200px;

    rect1 := Rectangle {
        color: black;
    }
    

    property <bool> rect1_pos_ok: rect1.x == 150px;
}
