{
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/better-docs",
    "destination": "./docs"
  },
  "templates": {
    "default": {
      "outputSourceFiles": false
    },
    "better-docs": {
      "name": "SixtyFPS Node Documentation",
      "hideGenerator": true,
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/sixtyfpsui/sixtyfps"
        }
      ]
    }
  }
}