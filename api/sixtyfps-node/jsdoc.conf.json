{
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/typescript"
  ],
  "opts": {
    "template": "node_modules/better-docs",
    "destination": "./docs"
  },
  "templates": {
    "default": {
      "outputSourceFiles": false
    },
    "better-docs": {
      "name": "SixtyFPS Node Documentation",
      "hideGenerator": true,
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/sixtyfpsui/sixtyfps"
        }
      ]
    }
  },
  "tags": {
    "allowUnknownTags": [
      "optional"
    ]
  },
  "source": {
    "includePattern": "\\.(jsx|js|ts|tsx)$"
  }
}