Xxx := Rectangle {
    signal plop(string, color, int);
    property <color> glop_col;
    property <string> blah: "yo";
    plop(x, blah, hello) => {
        color = blah;
        x = 42 + hello;
//      ^error{Assignement need to be done on a property}
        width = x;
//             ^error{Cannot convert string to length}
    }

    x: 12px;
}
