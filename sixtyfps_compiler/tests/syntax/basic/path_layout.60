X := Rectangle {

    PathLayout {
        for item[index] in [
            {name: "First Item"},
            {name: "Second Item"}
        ] : Text {
            text: item.name;
        }

        x: 50px;
        y: 50px;

        LineTo {
            x: 300;
            y: 300;
        }
    }

}